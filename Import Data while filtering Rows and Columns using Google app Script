function filter_rows_column() {

  var new_sheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName("Sheet1");                                  // Sheet where data is to be pasted
  var data_sheet = SpreadsheetApp.openById("1L8b5wereHeWrDPZjJdhasgfhjwfewrytewpBY").getSheetByName("data_sheet"); // Source Sheet
  new_sheet.getRange("A2:K").clear()

  var full_data  = data_sheet.getRange("A1:K").getValues();          // Taking range from source sheet
  var new_data = [];
  
  for (var i= 0; i<full_data.length;i++){

    var value = [];
      if(full_data[i][10] == "Vendor Payment"){       // Filtering Column & Filter values. for column K = [i][10] 
        value.push(full_data[i][0])                   // Columns which needed to be pasted. Index number for columns
        value.push(full_data[i][1])
        value.push(full_data[i][2])
        value.push(full_data[i][3])
        value.push(full_data[i][4])
        value.push(full_data[i][5])
        value.push(full_data[i][6])
        value.push(full_data[i][7])
        value.push(full_data[i][8])
        value.push(full_data[i][10])

     }
   if (value.length > 0){
     new_data.push(value)
   }
  }

  new_sheet.getRange(2,1,new_data.length,new_data[0].length).setValues(new_data)          // Pasting the fitered data into the destination sheet

}
